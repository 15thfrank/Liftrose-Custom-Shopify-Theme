                        var list1 = document.getElementById('brand');
                        var list2 = document.getElementById("model");
                        var list1SelectedValue = list1.options[list1.selectedIndex].value;

                        if (list1SelectedValue == 'Aeiomed') {

                            list2.options.length = 0;
                            list2.options[0] = new Option('--Select Model--', '');
                            list2.options[1] = new Option('Everest', 'Everest');

                        } else if (list1SelectedValue == 'Apex_Medical') {

                            list2.options.length = 0;
                            list2.options[0] = new Option('--Select Model--', '');
                            list2.options[1] = new Option('ICH', 'ICH');
                            list2.options[2] = new Option('XT', 'XT');

                        } else if (list1SelectedValue == 'Carefusion') {

                            list2.options.length = 0;
                            list2.options[0] = new Option('--Select Model--', '');
                            list2.options[1] = new Option('PureSom', 'PureSom');

                        } else if (list1SelectedValue == 'Devilbiss') {

                            list2.options.length = 0;
                            list2.options[0] = new Option('--Select Model--', '');
                            list2.options[1] = new Option('IntelliPAP', 'IntelliPAP');

