                            list2.options.length = 0;
                            list2.options[0] = new Option('--Select Model--', '');
                            list2.options[1] = new Option('ComfortPAP', 'ComfortPAP');

                        } else if (list1SelectedValue == 'Fisher_Paykel') {

                            list2.options.length = 0;
                            list2.options[0] = new Option('--Select Model--', '');
                            list2.options[1] = new Option('Icon', 'Icon_');
                            list2.options[2] = new Option('SleepStyle 200 Series', 'SleepStyle_200_Series');
                            list2.options[3] = new Option('SleepStyle 600 Series', 'SleepStyle_600_Series');

                        } else if (list1SelectedValue == 'Philips_Respironics') {

